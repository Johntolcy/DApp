<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.dapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="DApp"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Login_App"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".MainAll"
            android:label="糖App"
            >
        </activity>
        <activity
            android:name=".FruitMainActivity"
            android:label="饮食管理"
            android:windowSoftInputMode="adjustPan"></activity>
        <activity android:name=".FruitSelect"></activity>
        <activity android:name=".FruitSearch"></activity>
        <activity android:name=".SearchNullResult"></activity>
        <activity android:name=".Register_main"
            android:label="用户注册"></activity>

    </application>

</manifest>